    <h1>Estado del agua</h1>
<script>

var num1=parseInt(prompt("dime la temperatura"))

if (num1>0 && num1<100){
document.write("es liquido")
}
else if (num1>100)
document.write("es gaseoso")

if (num1<0)
document.write("es solido")



    
</script>




